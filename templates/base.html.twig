        {% set get_active_theme = master_theme.findOneBy({'status':1}) %}
        {% set get_theme_setting = [] %}
        {% if get_active_theme is defined and get_active_theme.id is defined %}
            {% set get_theme_setting = theme_setting.findBy({'master_theme':get_active_theme.id}) %}
        {% endif %}
        {% set get_theme_setting_array = [] %}
        {% for ts_data in get_theme_setting %}
            {% set get_theme_setting_array = get_theme_setting_array|merge({ (ts_data.name) :ts_data.value}) %}
        {% endfor %}
        {% set site_header_background = (get_theme_setting_array['header_background'] is defined and get_theme_setting_array['header_background']!='')? aws_s3_custom_uri_prefix~'/'~get_theme_setting_array['header_background']:'' %}
        {% set site_body_background = (get_theme_setting_array['body_background'] is defined and get_theme_setting_array['body_background']!='')? aws_s3_custom_uri_prefix~'/'~get_theme_setting_array['body_background']:'' %}
        {% set site_footer_background = (get_theme_setting_array['footer_background'] is defined and get_theme_setting_array['footer_background']!='')? aws_s3_custom_uri_prefix~'/'~get_theme_setting_array['footer_background']: asset('images/footer-border.png') %}
        {% set header_background_type = get_theme_setting_array['header_background_type']??'' %}
        {% set body_background_type = get_theme_setting_array['body_background_type']??'' %}
        {% set footer_background_type = get_theme_setting_array['footer_background_type']??'' %}
        {% set font_type = get_theme_setting_array['font_type']??'' %}

        {% set current_path = app.request.get('_route') %}
        <style>
            :root {
                --site_header_color: {{ (get_theme_setting_array['header_color'] is defined and get_theme_setting_array['header_color']!='')?get_theme_setting_array['header_color']:'black' }};
                --site_body_color: {{ (get_theme_setting_array['body_color'] is defined and get_theme_setting_array['body_color']!='')?get_theme_setting_array['body_color']:'black' }};
                --site_footer_color: {{ (get_theme_setting_array['footer_color'] is defined and get_theme_setting_array['footer_color']!='')?get_theme_setting_array['footer_color']:'black' }};
                --site_color: {{ (get_theme_setting_array['color'] is defined and get_theme_setting_array['color']!='')?get_theme_setting_array['color']:'white' }};
                --site_border_color: {{ (get_theme_setting_array['border_color'] is defined and get_theme_setting_array['border_color']!='')?get_theme_setting_array['border_color']:'#6441A5' }};
                --site_border_width: {{ (get_theme_setting_array['border'] is defined and get_theme_setting_array['border']!='')?get_theme_setting_array['border']:'1' }}px;
                --border_radius_top_left: {{ (get_theme_setting_array['border_radius_top_left'] is defined and get_theme_setting_array['border_radius_top_left']!='')?get_theme_setting_array['border_radius_top_left']:'20' }}px;
                --border_radius_top_right: {{ (get_theme_setting_array['border_radius_top_right'] is defined and get_theme_setting_array['border_radius_top_right']!='')?get_theme_setting_array['border_radius_top_right']:'5' }}px;
                --border_radius_bottom_right: {{ (get_theme_setting_array['border_radius_bottom_right'] is defined and get_theme_setting_array['border_radius_bottom_right']!='')?get_theme_setting_array['border_radius_bottom_right']:'20' }}px;
                --border_radius_bottom_left: {{ (get_theme_setting_array['border_radius_bottom_left'] is defined and get_theme_setting_array['border_radius_bottom_left']!='')?get_theme_setting_array['border_radius_bottom_left']:'20' }}px;
                --site_font_family: {{ (get_theme_setting_array['font_family'] is defined and get_theme_setting_array['font_family']!='')?get_theme_setting_array['font_family']:'' }};
                --site_remote_font_name: {{ (get_theme_setting_array['remote_font_name'] is defined and get_theme_setting_array['remote_font_name']!='')?get_theme_setting_array['remote_font_name']:'' }};
                --site_arrow_color: {{ (get_theme_setting_array['arrow_color'] is defined and get_theme_setting_array['arrow_color']!='')?get_theme_setting_array['arrow_color']:'#fff' }};
                --site_numbered_row_color: {{ (get_theme_setting_array['numbered_row_color'] is defined and get_theme_setting_array['numbered_row_color']!='')?get_theme_setting_array['numbered_row_color']:'#bababa' }};
                --site_numbered_row_outline_color: {{ (get_theme_setting_array['numbered_row_outline_color'] is defined and get_theme_setting_array['numbered_row_outline_color']!='')?get_theme_setting_array['numbered_row_outline_color']:'#595959' }};
                --site_twitch_glow_color: {{ (get_theme_setting_array['twitch_glow_color'] is defined and get_theme_setting_array['twitch_glow_color']!='')?get_theme_setting_array['twitch_glow_color']:'rgba(100, 65, 165, 0.5)' }};
                --site_youtube_glow_color: {{ (get_theme_setting_array['youtube_glow_color'] is defined and get_theme_setting_array['youtube_glow_color']!='')?get_theme_setting_array['youtube_glow_color']:'rgba(255, 0, 0, 0.6)' }};
                --site_privacy_background_color: {{ (get_theme_setting_array['privacy_background_color'] is defined and get_theme_setting_array['privacy_background_color']!='')?get_theme_setting_array['privacy_background_color']:'#000' }};
                --site_privacy_label_font_color: {{ (get_theme_setting_array['privacy_label_font_color'] is defined and get_theme_setting_array['privacy_label_font_color']!='')?get_theme_setting_array['privacy_label_font_color']:'#e11616' }};
                --site_privacy_regular_font_color: {{ (get_theme_setting_array['privacy_regular_font_color'] is defined and get_theme_setting_array['privacy_regular_font_color']!='')?get_theme_setting_array['privacy_regular_font_color']:'#6441A5' }};
				--site_mobile_embed_height: {{ (get_theme_setting_array['mobile_embed_height'] is defined and get_theme_setting_array['mobile_embed_height']!='')?get_theme_setting_array['mobile_embed_height']:'311' }}px;
            }
            .text-white, .text-grey, .elementor-button-text {
                color: var(--site_color);
            }
            .text-stroke {
                color: var(--site_numbered_row_color);
            }
            .text-stroke::before {
                -webkit-text-stroke-color: var(--site_numbered_row_outline_color);
            }
            .cut-edge__wrapper--twitch {
                filter: drop-shadow(0px 0px 6px var(--site_twitch_glow_color));
            }
            .cut-edge__wrapper--youtube {
                filter: drop-shadow(0px 0px 10px var(--site_youtube_glow_color));
            }

            .elementor-button, .elementor-button-x, .query_input {
                border-style: solid;
                border-color: var(--site_border_color) !important;
                border-width: var(--site_border_width);
                border-radius: var(--border_radius_top_left) var(--border_radius_top_right) var(--border_radius_bottom_right) var(--border_radius_bottom_left);
            }

            .query_input > button > svg > path {
                fill: var(--site_color);
            }
            .query_input:after {
                background-color: unset;
            }

            .slider_arrow > svg > path {
                fill: var(--site_arrow_color);
                stroke: var(--site_arrow_color);
            }
            .slider_arrow_big > svg > path {
                fill: var(--site_arrow_color);
            }
            {% if(header_background_type == 'color') %}
                header {
                    background-color: var(--site_header_color);
                }
            {% elseif(header_background_type == 'image') %}
                header {
                    position: relative;
                }
                .header_background {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    z-index: -1;
                }
            {% endif %}

            {% if(body_background_type == 'color') %}
                #app-body {
                    background-color: var(--site_body_color);
                }
            {% elseif(body_background_type == 'image') %}
                #app-body {
                    background-image: url("{{ site_body_background }}");
                    background-size: cover;
                    background-repeat: no-repeat;
                    background-position: center;
                }
            {% endif %}

            {% if(footer_background_type == 'color') %}
                footer {
                    background-color: var(--site_footer_color);
                }
            {% elseif(footer_background_type == 'image') %}
                footer {
                    background-image: url("{{ site_footer_background }}");
                    background-size: 100% 100%;
                    background-repeat: no-repeat;
                }
            {% endif %}

            {% if(current_path == 'terms' or current_path == 'privacy_policy') %}
                #app-body {
                    background-color: var(--site_privacy_background_color);
                    background-image: none;
                }

                #app-body .text-red-600,
                #app-body .text-xl,
                #app-body strong,
                #app-body h4,#app-body h5,
                #app-body li::marker,
                #app-body .text-heading
                {
                    color: var(--site_privacy_label_font_color);
                }

                #app-body table th,
                #app-body table td {
                    border-color: var(--site_privacy_regular_font_color);
                }
                #app-body p,
                #app-body .list-inside,
                #app-body table {
                    color: var(--site_privacy_regular_font_color);
                }
				#iubenda_policy .box_primary {
					background-color: rgb(0 6 18 / var(--tw-bg-opacity));
					border:none;
				}
				#iubenda_policy h1, #iubenda_policy h2, #iubenda_policy h3 {
					color: var(--site_privacy_label_font_color);
				}
            {% endif %}

            .custom-smooth-scroll {
                overflow-x: auto;
                display: flex;
                flex-wrap: nowrap;
                scroll-behavior: auto;
                cursor: all-scroll;
            }

            .custom-smooth-scroll::-webkit-scrollbar {
                display: none;
            }
            .custom-embed-m{
                height: var(--site_mobile_embed_height);
            }
            @media (max-width: 767px) {
                .custom-header-btn .elementor-button-text {
                    font-size: 10px;
                }
                .custom-header-btn {
                    border:none;
                    padding:3px;
                    height:auto
                }
            }
        </style>

        {% if(font_type == 'remote' and get_theme_setting_array['remote_font_url'] is defined and get_theme_setting_array['remote_font_url'] !='') %}
            <link rel="preconnect" href="https://fonts.googleapis.com">
            <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
            <link href="{{ get_theme_setting_array['remote_font_url']??'' }}" rel="stylesheet">
            <style>
                body {
                    font-family: var(--site_remote_font_name);
                }
            </style>
        {% elseif(font_type == 'local'  and get_theme_setting_array['font_family'] is defined and get_theme_setting_array['font_family'] !='') %}
            <style>
                body {
                    font-family: var(--site_font_family);
                }
            </style>
        {% endif %}

